{
  "name": "copy-of-qgo-job-file-management-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:client",
    "dev:client": "vite",
    "dev:server": "node --loader ts-node/esm server/index.ts",
    "dev:all": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "vite build && npm run build:server",
    "build:server": "tsc server/index.ts server/**/*.ts --outDir dist --module commonjs --target es2020 --declaration",
    "start:prod": "node dist/server/index.js",
    "preview": "vite preview",
    "start": "npm run build:server && npm run start:prod",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "react-dom": "^19.2.0",
    "react": "^19.2.0",
    "@google/genai": "^1.23.0",
    "chart.js": "^4.5.0",
    "signature_pad": "^5.1.1",
    "express": "^4.18.2",
    "@prisma/client": "^5.8.0",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "concurrently": "^8.2.2"
  }
}
